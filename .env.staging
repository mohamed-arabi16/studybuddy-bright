# StudyBuddy Staging Environment Configuration
# This file should be used for the staging environment
# Copy to .env.local when deploying to staging

# ============================================
# SUPABASE CONFIGURATION - STAGING
# ============================================
VITE_SUPABASE_URL="https://YOUR_STAGING_PROJECT_ID.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="your-staging-anon-key"
VITE_SUPABASE_PROJECT_ID="YOUR_STAGING_PROJECT_ID"

# ============================================
# ERROR TRACKING (Sentry)
# ============================================
# Sentry DSN - use same project but environment tag distinguishes
VITE_SENTRY_DSN="https://your-sentry-dsn@sentry.io/project-id"

# ============================================
# ENVIRONMENT INDICATOR
# ============================================
VITE_APP_ENV="staging"
